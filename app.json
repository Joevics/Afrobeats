{
  "expo": {
    "name": "AfroBeats Quiz",
    "slug": "afrobeats-quiz",
    "owner": "joevics1",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "afrobeats-quiz",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Test your knowledge of African music with AfroBeats Quiz! Challenge yourself with songs, artists, and lyrics from across the continent.",
    "keywords": [
      "afrobeats",
      "music",
      "quiz",
      "africa",
      "entertainment",
      "games"
    ],
    "privacy": "public",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.afrobeats.quiz",
      "buildNumber": "1.0.1",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app uses the microphone to play audio clips for the music quiz.",
        "NSCameraUsageDescription": "This app may use the camera for sharing quiz results."
      }
    },
    "android": {
      "package": "com.afrobeats.quiz",
      "versionCode": 2,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal"
      },
      "preview": {
        "distribution": "internal",
        "ios": {
          "resourceClass": "m-medium"
        },
        "android": {
          "resourceClass": "medium"
        }
      },
      "production": {
        "distribution": "store",
        "ios": {
          "resourceClass": "m-medium"
        },
        "android": {
          "resourceClass": "medium"
        }
      }
    },
